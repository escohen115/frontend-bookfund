itlistMapped = waitingsMapped.map(waiting=>{\n            return (\n            <li>\n                <img src = {waiting.image_url }></img>\n                <Link to={`/bookpage/${waiting.api_id}`}>{waiting.title}</Link>\n            </li>\n            )\n        })\n\n    return(\n        <div> \n            <img src={user.profile_pic ? user.profile_pic : null}></img>\n            <p>name:{user.name ? user.name : null}</p>\n            <p>email:{user.email ? user.email : null}</p>\n            <p>username:{user.username ? user.username : null }</p>\n            <p>bio:{user.bio ? user.bio : null}</p>\n            <p>waitlist</p>\n            <ul>{waitingsMapped.length>0 ? waitlistMapped:null}</ul>\n            <p>sponsored list</p>\n            <p>received list</p>\n        </div>\n    )\n    }\n    else{\n        return null\n    }\n}","/Users/simoncohen/Flatiron/code/Mod5/bookfund/frontend-bookfund/bookfund/src/components/pages/BookPage/SponsorForm.js",[],"/Users/simoncohen/Flatiron/code/Mod5/bookfund/frontend-bookfund/bookfund/src/components/pages/OtherUserPage/OtherUserPage.js",["87","88"],"import { useParams } from \"react-router-dom\";\nimport React, { useEffect, useState } from \"react\"\n\nexport default function OtherUserPage(){\n\n    const [otherUser, setOtherUser] = useState(null)\n    \n    const params = useParams()\n    useEffect(()=>{\n        fetch(`http://localhost:3000/users/${params.id}`)\n        .then(response => response.json())\n        .then(data=>setOtherUser(data))  \n    },[])\n\n    if (otherUser){\n        return(\n            <div> \n                <img src={otherUser.profile_pic ? otherUser.profile_pic : null}></img>\n                <p>name: {otherUser.name ? otherUser.name : null}</p>\n                <p>email: {otherUser.email ? otherUser.email : null}</p>\n                <p>username: {otherUser.username ? otherUser.username : null }</p>\n                <p>bio: {otherUser.bio ? otherUser.bio : null}</p>\n            </div>\n  